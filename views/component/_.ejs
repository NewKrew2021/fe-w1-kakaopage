<!--
{
  type: string
  data: json
}
or
{
  components: array of above
}
-->
<!--
  BUGGY: if you switch the order of below if-statement, it would not work
-->
<% if (typeof components !== 'undefined') { %>
  <!-- multiple components -->
  <% for (const { type, data } of components) { %>
    <%- include(`_${type}`, data) %>
  <% } %>
<% } else if (typeof type !== 'undefined' && typeof data !== 'undefined') { %>
  <!-- single component -->
  <%- include(`_${type}`, data) %>
<% } %>
